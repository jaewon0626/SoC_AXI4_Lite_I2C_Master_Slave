<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="i2c_cpu_wrapper"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
i2c_cpu_wrapper
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="i2c_cpu_wrapper">
        <sdx:configuration sdx:name="i2c_cpu_wrapper"
                           sdx:displayName="i2c_cpu_wrapper"
                           sdx:defaultProcessorGroup="standalone_microblaze_0"
                           sdx:runtimes="cpp">
            <sdx:description>i2c_cpu_wrapper</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="i2c_cpu_wrapper/boot/i2c_cpu_wrapper.bif"
                            sdx:readme="i2c_cpu_wrapper/boot/generic.readme"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_microblaze_0"
                                sdx:displayName="standalone_microblaze_0"
                                sdx:cpuType="microblaze_0"
                                sdx:cpuInstance="microblaze_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="i2c_cpu_wrapper/standalone_microblaze_0/bsplib/lib"
                        sdx:bspIncludePaths="i2c_cpu_wrapper/standalone_microblaze_0/bspinclude/include"
                        sdx:bspConfig="i2c_cpu_wrapper/standalone_microblaze_0/system.mss"
                />
            </sdx:processorGroup>
        </sdx:configuration>
    </sdx:systemConfigurations>
</sdx:platform>
